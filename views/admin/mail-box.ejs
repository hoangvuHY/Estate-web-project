    <!-- start: Content -->
    <div id="content">
      <div class="col-md-12" style="padding:20px;">
        <div class="chat-container">
          <header class="chat-header">
            <h1>Chat Trực tiếp</h1>
            <!-- <a href="/" class="btn">Leave Room</a> -->
          </header>
          <main class="chat-main">
            <div class="chat-sidebar">
              <h3><i class="fas fa-comments"></i> Tên phòng:</h3>
              <h2 id="room-name"></h2>
              <h3><i class="fas fa-users"></i> Thành viên</h3>
              <ul id="users"></ul>
            </div>
            <div class="chat-messages"></div>
          </main>
          <div class="chat-form-container">
            <form id="chat-form">
              <input id="msg" type="text" placeholder="Nhập tin nhắn" required autocomplete="off" />
              <button class="btn"><i class="fas fa-paper-plane"></i> Gửi</button>
            </form>
          </div>
        </div>

      </div>
    </div>
    <!-- end: Content -->

