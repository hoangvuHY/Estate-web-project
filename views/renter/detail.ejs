<body>
  <!-- Thanh đăng nhập, đăng ký -->
  <div class="bg-dark" id="parent-login-subcrible">
    <div class="container text-right p-1" id="tab-login-subcrible">

    </div>
  </div>

  <!-- Menu các chuyên mục -->
  <div id="menu" class="sticky-top">
    <div class="container d-flex justify-content-center align-items-center p-1">
      <div class="row w-100 w-100">
        <div class="col-md-9 col-lg-5">
          <div id="logo-sologan" class="d-flex justify-content-center align-items-center">
            <img id="logo" src="../images/logo.png" alt="">
            <div id='sologan'>I TRUST YOU 3000</div>
          </div>
        </div>
        <div class="col-md-3 col-lg-7 p-0">
          <div id="menus" class="h-100">
            <nav class="navbar navbar-expand-lg pr-0 h-100 navbar-light">
              <div class="navbar-brand d-block d-lg-none">Menu</div>
              <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup"
                aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
              </button>
              <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                <div class="navbar-nav d-flex w-100 justify-content-end">
                  <a class="nav-item nav-link" href="/">TRANG CHỦ</a>
