                  <a class="nav-item nav-link" href="#">TIN TỨC VÀ SỰ KIỆN</a>
                  <a class="nav-item nav-link" href="#">TÀI KHOẢN</a>
                </div>
              </div>
            </nav>
          </div>
        </div>
      </div>
    </div>
  </div>


  <div class="container">
    <div class="row parent">

      <div class="col-md-4 child1 wow bounceInLeft">

        <div class="row image-group">
          <!--<a href="/public/images/vu.jpg" data-toggle="lightbox" data-gallery="gallery" class="col-md-12">
                <img src="/public/images/vu.jpg" class="img-fluid rounded">
                </a>
                <a href="/public/images/tuan.jpg" data-toggle="lightbox" data-gallery="gallery" class="col-md-6">
                <img src="/public/images/tuan.jpg" class="img-fluid rounded">
                </a>
                <a href="/public/images/thuan1.jpg" data-toggle="lightbox" data-gallery="gallery" class="col-md-6">
                <img src="/public/images/thuan1.jpg" class="img-fluid rounded">
                </a> -->
        </div>

        <div class="info-owner">
          <h1 class="text-center">THÔNG TIN LIÊN HỆ</h1>
          <div class="row">
            <div class="col-5 color">Họ tên</div>
            <div class="col-7" id="name-owner"></div>
          </div>
          <div class="row">
            <div class="col-5 color">Điện thoại</div>
            <div class="col-7" id="telephone-owner"></div>
          </div>
          <div class="row">
            <div class="col-5 color">Email</div>
            <div class="col-7" id="email-owner"></div>
          </div>
          <div class="row">
            <div class="col-5 color">Ngày đăng</div>
            <div class="col-7" id="date-start"></div>
          </div>
          <div class="row">
            <div class="col-5 color">Ngày hết hạn</div>
            <div class="col-7" id="date-end"></div>
          </div>

        </div>

        <div class="report">
        </div>

      </div>


      <div class="col-md-8 child2 wow bounceInRight">

        <div class="title">
          <h1 id="name">PHÒNG TRỌ</h1>
          <div class="color"><i class="fas fa-map-marker-alt"></i><span id="address"></span></div>
          <div class="color"><i class="fas fa-home"></i><span id="status"></span></div>
        </div>

        <div class="info">
          <h1>TỔNG QUÁT</h1>
          <div class="row">
            <div class="col-6 col-md-3 color"><i class="fas fa-shield-alt"></i>An ninh</div>
            <div class="col-6 col-md-3" id="an-ninh"></div>
            <div class="col-6 col-md-3 color"><i class="far fa-square"></i>Diện tích</div>
            <div class="col-6 col-md-3"><span id="dien-tich"></span> m<sup>2</sup></div>
          </div>

          <div class="row">
            <div class="col-6 col-md-3 color"><i class="fas fa-bolt"></i>Tiền điện</div>
            <div class="col-6 col-md-3"><span id="tien-dien"></span> VNĐ/kWh</sup></div>
            <div class="col-6 col-md-3 color"><i class="fas fa-tint"></i>Tiền nước</div>
            <div class="col-6 col-md-3"><span id="tien-nuoc"></span> VNĐ/m<sup>3</sup></div>
          </div>
          <div class="row">
            <div class="col-6 col-md-3 color"><i class="fab fa-houzz"></i>Số phòng</div>
            <div class="col-6 col-md-3"><span id="so-phong"></span> phòng</div>
            <div class="col-6 col-md-3 color"><i class="fas fa-money-bill-wave-alt"></i>Tiền thuê</div>
            <div class="col-6 col-md-3"><span id="tien-thue"></span> VNĐ</div>
          </div>
        </div>

        <div class="utility">
          <h1>TIỆN ÍCH</h1>
          <div class="row">
            <div class="col-sm-1"></div>
            <div class="col-sm-2 col-4" id="ban-cong">
              <img class="img-fluid w-50" src="/images/ban-cong.png" alt="">
            </div>
            <div class="col-sm-2 col-4" id="dieu-hoa">
              <img class="img-fluid w-50 " src="/images/dieu-hoa.png" alt="">
            </div>
            <div class="col-sm-2 col-4" id="binh-nong-lanh">
              <img class="img-fluid w-50" src="/images/binh-nong-lanh.png" alt="">
            </div>
            <div class="col-sm-2 col-4" id="tu-lanh">
              <img class="img-fluid w-25" src="/images/tu-lanh.png" alt="">
            </div>
            <div class="col-sm-2 col-4" id="may-giat">
              <img class="img-fluid w-50" src="/images/may-giat.png" alt="">
            </div>
            <div class="col-sm-1"></div>
          </div>

        </div>

        <div class="map">
          <h1>BẢN ĐỒ</h1>
          <div>
            <iframe
              src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d14895.444953462416!2d105.7827076!3d21.0382375!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x5d0313a3bfdc4f37!2zVHLGsOG7nW5nIMSQ4bqhaSBo4buNYyBDw7RuZyBuZ2jhu4csIMSQ4bqhaSBo4buNYyBRdeG7kWMgZ2lhIEjDoCBO4buZaQ!5e0!3m2!1svi!2s!4v1608538791545!5m2!1svi!2s"
              width="600" height="450" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false"
              tabindex="0"></iframe>
          </div>
        </div>

        <div class="bar">
          <div class="row">
            <div class="col-4 color" id="like">
              <i class="far fa-heart"></i><span>Yêu thích</span>
            </div>
            <div class="col-4 color">
              <i class="far fa-comment-alt"></i><span>Bình luận</span>
            </div>
            <div class="col-4 color">
              <i class="fas fa-share"></i><span>Chia sẻ</span>
            </div>
          </div>
          <div class="row " id="renterComments">

            <div class="comment">
              <img src="/public/images/user-avatar.png" alt="">
              <div class="my-comment w-100">
                <div class='ratings '>
                  <span class="fa fa-star   star  "></span>
                  <span class="fa fa-star  star "></span>
                  <span class="fa fa-star  star "></span>
                  <span class="fa fa-star  star"></span>
                  <span class="fa fa-star  star"></span>
                </div>
                <textarea id="comment-render" placeholder="Viết bình luận của bạn công khai"></textarea>
                <div class="send" id="send-comment">SEND</div>
              </div>
            </div>


          </div>
        </div>

      </div>

    </div>

  </div>

